{
    "$schema": "https://json.schemastore.org/tsconfig",
    "compilerOptions": {
      "isolatedModules": false,
      "moduleResolution": "node",
      "esModuleInterop": true,
      "baseUrl": ".",
      "paths": {
        "@/*": ["./*"]
      }
    },
    "include": ["src/**/*.ts"],
    "exclude": ["node_modules","components"]
}